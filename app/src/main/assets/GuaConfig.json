[{"desc":"刚健中正","desc_group":"乾为天","detail":"元亨，利贞。","id":1,"image":"1,1,1,1,1,1","name":"乾"},{"desc":"柔顺伸展","desc_group":"坤为地","detail":"元亨，利牝马之贞。君子有攸往，先迷后得主，利西南得朋，东北丧朋。安贞，吉。","id":2,"image":"0,0,0,0,0,0","name":"坤"},{"desc":"起始维艰","desc_group":"水雷屯","detail":"元亨，利贞，勿用有攸往，利建侯。","id":3,"image":"0,1,0,0,0,1","name":"屯"},{"desc":"启蒙奋发","desc_group":"山水蒙","detail":"亨。匪我求童蒙，童蒙求我。初筮告，再三渎，渎则不告。利贞。","id":4,"image":"1,0,0,0,1,0","name":"蒙"},{"desc":"守正待机","desc_group":"水天需","detail":"有孚，光亨，贞吉。利涉大川。","id":5,"image":"0,1,0,1,1,1","name":"需"},{"desc":"慎争戒讼","desc_group":"天水讼","detail":"有孚，窒。惕中吉。终凶。利见大人，不利涉大川。","id":6,"image":"1,1,1,0,1,0","name":"讼"},{"desc":"行险而顺","desc_group":"地水师","detail":"贞，丈人，吉无咎。","id":7,"image":"0,0,0,0,1,0","name":"师"},{"desc":"诚信团结","desc_group":"水地比","detail":"吉。原筮元永贞，无咎。不宁方来，后夫凶。","id":8,"image":"0,1,0,0,0,0","name":"比"},{"desc":"蓄养待进","desc_group":"风天小畜","detail":"亨。密云不雨，自我西郊。","id":9,"image":"1,1,0,1,1,1","name":"小畜"},{"desc":"脚踏实地","desc_group":"天泽履","detail":"履虎尾，不咥人，亨。","id":10,"image":"1,1,1,0,1,1","name":"履"},{"desc":"应时而变","desc_group":"地天泰","detail":"小往大来，吉亨。","id":11,"image":"0,0,0,1,1,1","name":"泰"},{"desc":"不交不通","desc_group":"天地否","detail":"否之匪人，不利君子贞，大往小来。","id":12,"image":"1,1,1,0,0,0","name":"否"},{"desc":"上下和同","desc_group":"天火同人","detail":"同人于野，亨。利涉大川，利君子贞。","id":13,"image":"1,1,1,1,0,1","name":"同人"},{"desc":"顺天依时","desc_group":"火天大有","detail":"元亨。","id":14,"image":"1,0,1,1,1,1","name":"大有"},{"desc":"内高外低","desc_group":"地山谦","detail":"亨，君子有终。","id":15,"image":"0,0,0,1,0,0","name":"谦"},{"desc":"顺时依势","desc_group":"雷地豫","detail":"利建侯，行师。","id":16,"image":"0,0,1,0,0,0","name":"豫"},{"desc":"随时变通","desc_group":"泽雷随","detail":"元亨利贞，无咎。","id":17,"image":"0,1,1,0,0,1","name":"随"},{"desc":"振疲起衰","desc_group":"山风蛊","detail":"元亨，利涉大川。先甲三日，后甲三日。","id":18,"image":"1,0,0,1,1,0","name":"蛊"},{"desc":"教民保民","desc_group":"地泽临","detail":"元，亨，利，贞。至于八月有凶。","id":19,"image":"0,0,0,0,1,1","name":"临"},{"desc":"观下瞻上","desc_group":"风地观","detail":"盥而不荐，有孚顒若。","id":20,"image":"1,1,0,0,0,0","name":"观"},{"desc":"刚柔相济","desc_group":"火雷噬嗑","detail":"亨。利用狱。","id":21,"image":"1,0,1,0,0,1","name":"噬嗑"},{"desc":"饰外扬质","desc_group":"山火贲","detail":"亨。小利有攸往。","id":22,"image":"1,0,0,1,0,1","name":"贲"},{"desc":"顺势而止","desc_group":"山地剥","detail":"不利有攸往。","id":23,"image":"1,0,0,0,0,0","name":"剥"},{"desc":"寓动于顺","desc_group":"地雷复","detail":"亨。出入无疾，朋来无咎。反复其道，七日来复，利有攸往。","id":24,"image":"0,0,0,0,0,1","name":"复"},{"desc":"无妄而得","desc_group":"天雷无妄","detail":"元亨，利贞。其匪正有眚，不利有攸往。","id":25,"image":"1,1,1,0,0,1","name":"无妄"},{"desc":"止而不止","desc_group":"山天大畜","detail":"利贞，不家食吉，利涉大川。","id":26,"image":"1,0,0,1,1,1","name":"大畜"},{"desc":"纯正以养","desc_group":"山雷颐","detail":"贞吉。观颐，自求口实。","id":27,"image":"1,0,0,0,0,1","name":"颐"},{"desc":"十分行动","desc_group":"泽风大过","detail":"栋桡，利有攸往，亨。","id":28,"image":"0,1,1,1,1,0","name":"大过"},{"desc":"行险用险","desc_group":"坎为水","detail":"习坎，有孚，维心亨，行有尚。","id":29,"image":"0,1,0,0,1,0","name":"坎"},{"desc":"附和依托","desc_group":"离为火","detail":"利贞，亨。畜牝牛，吉。","id":30,"image":"1,0,1,1,0,1","name":"离"},{"desc":"相互感应","desc_group":"泽山咸","detail":"咸，亨，利贞，取女吉。","id":31,"image":"0,1,1,1,0,0","name":"咸"},{"desc":"恒心有成","desc_group":"雷风恒","detail":"亨，无咎，利贞，利有攸往。","id":32,"image":"0,0,1,1,1,0","name":"恒"},{"desc":"遁世救世","desc_group":"天山遁","detail":"亨，小利贞。","id":33,"image":"1,1,1,1,0,0","name":"遯"},{"desc":"纯正以养","desc_group":"雷天大壮","detail":"利贞。","id":34,"image":"0,0,1,1,1,1","name":"大壮"},{"desc":"求进发展","desc_group":"火地晋","detail":"康侯用锡马蕃庶，昼日三接。","id":35,"image":"1,0,1,0,0,0","name":"晋"},{"desc":"晦而转明","desc_group":"地火明夷","detail":"利艰贞。","id":36,"image":"0,0,0,1,0,1","name":"明夷"},{"desc":"诚威治业","desc_group":"风火家人","detail":"利女贞。","id":37,"image":"1,1,0,1,0,1","name":"家人"},{"desc":"异中求同","desc_group":"火泽睽","detail":"小事吉。","id":38,"image":"1,0,1,0,1,1","name":"睽"},{"desc":"险阻在前","desc_group":"水山蹇","detail":"利西南，不利东北；利见大人，贞吉。","id":39,"image":"0,1,0,1,0,0","name":"蹇"},{"desc":"柔道致治","desc_group":"雷水解","detail":"利西南，无所往，其来复吉。有攸往，夙吉。","id":40,"image":"0,0,1,0,1,0","name":"解"},{"desc":"损益制衡","desc_group":"山泽损","detail":"损：有孚，元吉，无咎，可贞，利有攸往。曷之用，二簋可用享。","id":41,"image":"1,0,0,0,1,1","name":"损"},{"desc":"损上益下","desc_group":"风雷益","detail":"利有攸往，利涉大川。","id":42,"image":"1,1,0,0,0,1","name":"益"},{"desc":"决而能和","desc_group":"泽天夬","detail":"扬于王庭，孚号，有厉，告自邑，不利即戎，利有攸往。","id":43,"image":"0,1,1,1,1,1","name":"夬"},{"desc":"天下有风","desc_group":"天风姤","detail":"女壮，勿用取女。","id":44,"image":"1,1,1,1,1,0","name":"姤"},{"desc":"荟萃聚集","desc_group":"泽地萃","detail":"亨。王假有庙，利见大人，亨，利贞。用大牲吉，利有攸往。","id":45,"image":"0,1,1,0,0,0","name":"萃"},{"desc":"柔顺谦虚","desc_group":"地风升","detail":"元亨，用见大人，勿恤，南征吉。","id":46,"image":"0,0,0,1,1,0","name":"升"},{"desc":"困境求通","desc_group":"泽水困","detail":"亨，贞，大人吉，无咎，有言不信。","id":47,"image":"0,1,1,0,1,0","name":"困"},{"desc":"求贤若渴","desc_group":"水风井","detail":"改邑不改井，无丧无得，往来井井。汔至，亦未繘井，羸其瓶，凶。","id":48,"image":"0,1,0,1,1,0","name":"井"},{"desc":"顺天应人","desc_group":"泽火革","detail":"巳日乃孚，元亨利贞，悔亡。","id":49,"image":"0,1,1,1,0,1","name":"革"} ,{"desc":"稳重图变","desc_group":"火风鼎","detail":"元吉，亨。","id":50,"image":"1,0,1,1,1,0","name":"鼎"},{"desc":"临危不乱","desc_group":"震为雷","detail":"亨。震来虩虩，笑言哑哑。震惊百里，不丧匕鬯。","id":51,"image":"0,0,1,0,0,1","name":"震"},{"desc":"动静适时","desc_group":"艮为山","detail":"艮其背，不获其身，行其庭，不见其人，无咎。","id":52,"image":"1,0,0,1,0,0","name":"艮"},{"desc":"渐进蓄德","desc_group":"风山渐","detail":"女归吉，利贞。","id":53,"image":"1,1,0,1,0,0","name":"渐"},{"desc":"立家兴业","desc_group":"雷泽归妹","detail":"征凶，无攸利。","id":54,"image":"0,0,1,0,1,1","name":"归妹"},{"desc":"日中则斜","desc_group":"雷火丰","detail":"亨，王假之，勿忧，宜日中。","id":55,"image":"0,0,1,1,0,1","name":"丰"},{"desc":"依义顺时","desc_group":"火山旅","detail":"小亨，旅贞吉。","id":56,"image":"1,0,1,1,0,0","name":"旅"},{"desc":"谦逊受益","desc_group":"巽为风","detail":"小亨，利攸往，利见大人。","id":57,"image":"1,1,0,1,1,0","name":"巽"},{"desc":"刚内柔外","desc_group":"兑为泽","detail":"亨，利贞。","id":58,"image":"0,1,1,0,1,1","name":"兑"},{"desc":"拯救涣散","desc_group":"风水涣","detail":"亨。王假有庙，利涉大川，利贞。","id":59,"image":"1,1,0,0,1,0","name":"涣"},{"desc":"万物有节","desc_group":"水泽节","detail":"亨。苦节，不可贞。","id":60,"image":"0,1,0,0,1,1","name":"节"},{"desc":"诚信立身","desc_group":"风泽中孚","detail":"豚鱼吉，利涉大川，利贞。","id":61,"image":"1,1,0,0,1,1","name":"中孚"},{"desc":"行动有度","desc_group":"雷山小过","detail":"亨，利贞，可小事，不可大事。飞鸟遗之音，不宜上，宜下，大吉。","id":62,"image":"0,0,1,1,0,0","name":"小过"},{"desc":"盛极将衰","desc_group":"水火既济","detail":"亨，小利贞，初吉终乱。","id":63,"image":"0,1,0,1,0,1","name":"既济"},{"desc":"事业未竟","desc_group":"火水未济","detail":"亨，小狐汔济，濡其尾，无攸利。","id":64,"image":"1,0,1,0,1,0","name":"未济"}]